{
    "name": "phlexible/task-bundle",
    "description": "phlexible task bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["tasks", "todo"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\TaskBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "yohang/finite": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "symfony/config": "^2.8",
        "symfony/dependency-injection": "^2.8",
        "symfony/http-kernel": "^2.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
