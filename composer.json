{
    "name": "phlexible/task-bundle",
    "description": "phlexible task bundle",
    "license": "proprietary",
    "type": "symfony-bundle",
    "keywords": ["tasks", "todo"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\TaskBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "yohang/finite": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-kernel": "~2.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/repo/private/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
